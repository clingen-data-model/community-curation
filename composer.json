{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/yugen/surveys"
    }],
    "require": {
        "php": "^8.2",
        "backpack/crud": "^6.0",
        "backpack/theme-coreuiv2": "^1.2",
        "barryvdh/laravel-elfinder": "^0.5.2",
        "box/spout": "^3.1",
        "doctrine/dbal": "^3.0",
        "erusev/parsedown": "^1.7",
        "lab404/laravel-impersonate": "^1.3",
        "laravel/framework": "^10.0",
        "laravel/helpers": "^1.3",
        "laravel/legacy-factories": "^1.1",
        "laravel/passport": "^11.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "league/html-to-markdown": "^4.9",
        "lorisleiva/laravel-actions": "^2.4",
        "mpdf/mpdf": "^8.0",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^2.0",
        "sirs/dbimport": "2.0.x-dev",
        "sirs/surveys": "4.0.x-dev",
        "spatie/calendar-links": "^1.3",
        "spatie/laravel-activitylog": "^4.0",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.5",
        "fakerphp/faker": "^1.9.1",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0",
        "spatie/ignition": "^1.11"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeds",
            "Database\\Factories\\": "database/factories"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "setup": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php artisan key:generate",
            "php artisan passport:keys",
            "php artisan migrate:fresh --seed",
            "npm install"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": ["php artisan storage:link --quiet"]
    }
}
