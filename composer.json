{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "backpack/crud": "^4.0.0",
        "barryvdh/laravel-elfinder": "^0.4.2",
        "box/spout": "^3.1",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.0",
        "lab404/laravel-impersonate": "^1.3",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.3",
        "laravel/passport": "^9.3.2",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/tinker": "^2.0",
        "league/html-to-markdown": "^4.9",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^1.2",
        "sirs/dbimport": "2.0.x-dev",
        "sirs/surveys": "4.0.9",
        "spatie/calendar-links": "^1.3",
        "spatie/laravel-activitylog": "^3.2",
        "spatie/laravel-permission": "^3.0",
        "yugen/laravel-db-mail-log": "dev-master"
    },
    "require-dev": {
        "backpack/generators": "^2.0",
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "deployer/dist": "^6.4",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "dev-master",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "spatie/phpunit-watcher": "^1.10"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Import/helpers.php",
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Import/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "setup": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php artisan key:generate",
            "php artisan passport:keys",
            "php artisan migrate:fresh --seed",
            "npm install"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}