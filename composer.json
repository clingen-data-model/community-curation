{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/yugen/surveys"
    }],
    "require": {
        "php": "^8.0",
        "backpack/crud": "^4.1.0",
        "barryvdh/laravel-elfinder": "^0.4.2",
        "box/spout": "^3.1",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "lab404/laravel-impersonate": "^1.3",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.3",
        "laravel/legacy-factories": "^1.0",
        "laravel/passport": "^10.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/html-to-markdown": "^4.9",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^1.2",
        "sirs/dbimport": "2.0.x-dev",
        "sirs/surveys": "^4.0.12",
        "spatie/calendar-links": "^1.3",
        "spatie/laravel-activitylog": "^3.2",
        "spatie/laravel-permission": "^3.0",
        "yugen/laravel-db-mail-log": "dev-master"
    },
    "require-dev": {
        "backpack/generators": "^3.0",
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.5",
        "deployer/dist": "^6.4",
        "facade/ignition": "^2.3.6",
        "filp/whoops": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laracasts/generators": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0",
        "spatie/phpunit-watcher": "^1.10"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeds",
            "Database\\Factories\\": "database/factories"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "setup": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php artisan key:generate",
            "php artisan passport:keys",
            "php artisan migrate:fresh --seed",
            "npm install"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}