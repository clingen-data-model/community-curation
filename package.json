        "sirs-skiptrigger": "^1.1.2",
        "timepicker": "^1.11.15",
        "vue": "^2.6.11",
        "vue-router": "^3.1.2",
        "vue-template-compiler": "^2.6.11",
        "vuex": "^3.1.1"
    },
